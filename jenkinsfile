pipeline{
    agent any

    stages{
         stage('Clean'){
                steps{
                    echo('Cleaning...')
                   sh 'clean'

                }
            }

          stage('Build'){
                steps{
                    echo('Building...')
                    sh 'assembleDebug'
                }
            }

             stage('Test'){
                    steps{
                        echo('Testing...')
                        sh 'testDebugUnitTest'
                    }
                }
         }
}
